From: Ray Donnelly <mingw.android@gmail.com>
Date: Tue, 23 Jun 2020 21:23:20 +0200
Subject: [PATCH] qtwebengine: link gn to librt

---
 gn/build/gen.py | 1 +
 1 file changed, 1 insertion(+)

diff --git a/qtwebengine/src/3rdparty/gn/gn/build/gen.py b/qtwebengine/src/3rdparty/gn/gn/build/gen.py
index b09d32ce842..3f9c913d0a7 100755
--- a/qtwebengine/src/3rdparty/gn/gn/build/gen.py
+++ b/qtwebengine/src/3rdparty/gn/gn/build/gen.py
@@ -366,6 +366,7 @@ def WriteGNNinja(path, platform, host, options):
 
       # This is needed by libc++.
       libs.append('-ldl')
+      libs.append('-lrt')
     elif platform.is_darwin():
       min_mac_version_flag = '-mmacosx-version-min=10.9'
       cflags.append(min_mac_version_flag)
